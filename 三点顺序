#include<stdio.h>
int main()
{
	int x1,x2,x3,y1,y2,y3;
	int item;
	
	scanf("%d %d",&x1,&y1);
	scanf("%d %d",&x2,&y2);
	scanf("%d %d",&x3,&y3);
	
	item = (x2-x1)*(y3-y1)-(y2-y1)*(x3-x1);
	if(item>0){
		printf("逆时针\n");
	}else if(item<0){
		printf("顺时针\n");
	}else{
		printf("三点共线\n");
	}
	
	return 0;
}
